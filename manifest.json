{
  "name": "Direct Parser",
  "version": "0.0.7",
  "manifest_version": 2,
  "description": "-",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "https://direct.yandex.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://direct.yandex.ru/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}